{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/user/tsconfig.app.json"
  },
  "projects": {
    "user": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "admin": {
      "type": "application",
      "root": "apps/admin",
      "entryFile": "main",
      "sourceRoot": "apps/admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin/tsconfig.app.json"
      }
    },

    "buddy": {
      "type": "application",
      "root": "apps/buddy",
      "entryFile": "main",
      "sourceRoot": "apps/buddy/src",
      "compilerOptions": {
        "tsConfigPath": "apps/buddy/tsconfig.app.json"
      }
    },

    "socket": {
      "type": "application",
      "root": "apps/socket",
      "entryFile": "main",
      "sourceRoot": "apps/socket/src",
      "compilerOptions": {
        "tsConfigPath": "apps/socket/tsconfig.app.json"
      }
    },
    "batch": {
