      }
    },
    "guard": {
      "type": "library",
      "root": "common/guard",
      "entryFile": "index",
      "sourceRoot": "common/guard/src",
      "compilerOptions": {
        "tsConfigPath": "common/guard/tsconfig.common.json"
      }
    },
    "modules": {
      "type": "library",
      "root": "common/modules",
      "entryFile": "index",
      "sourceRoot": "common/modules/src",
      "compilerOptions": {
        "tsConfigPath": "common/modules/tsconfig.common.json"
