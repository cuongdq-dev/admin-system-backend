      "root": "apps/batch",
      "entryFile": "main",
      "sourceRoot": "apps/batch/src",
      "compilerOptions": {
        "tsConfigPath": "apps/batch/tsconfig.app.json"
      }
    },
    "vps": {
      "type": "application",
      "root": "apps/vps",
      "entryFile": "main",
      "sourceRoot": "apps/vps/src",
      "compilerOptions": {
        "tsConfigPath": "apps/vps/tsconfig.app.json"
      }
    },

    "config": {
      "type": "library",
      "root": "common/config",
      "entryFile": "index",
      "sourceRoot": "common/config/src",
      "compilerOptions": {
        "tsConfigPath": "common/config/tsconfig.common.json"
      }
    },
    "crud": {
      "type": "library",
      "root": "common/crud",
      "entryFile": "index",
      "sourceRoot": "common/crud/src",
      "compilerOptions": {
        "tsConfigPath": "common/crud/tsconfig.common.json"
      }
    },
    "decorators": {
